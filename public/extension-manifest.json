{
  "manifest_version": 3,
  "name": "Cross Web Ar",
  "version": "0.1.0",
  "description": "Cross Web Ar Extension",
  "author": "Yeonwoo Cho",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon192.png",
    "48": "icon192.png",
    "128": "icon192.png"
  },
  "action": {
    "default_icon": {
      "16": "icon192.png",
      "48": "icon192.png"
    },
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["static/*"],
  "permissions": ["storage", "tabs", "activeTab", "scripting", "history"]
}
